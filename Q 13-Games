#include <iostream>
#include <bits/stdc++.h>
using namespace std;
 
int main() {
    int n; cin>>n;
    int a[n]; int b[n];
    for(int i=0; i<n; i++){
        int x,y; cin>>x>>y;
        a[i]=x; b[i]=y;
    }
    int cnt=0;
    for(int i=0; i<n; i++){
        for(int j=0; j<n; j++){
            if(a[i]==b[j]) cnt++;
        }
    }
    cout<<cnt;
}
