#include <iostream>
using namespace std;
#include <bits/stdc++.h>
int main() {
   int x,y,i,j,a[5][5],ans;
   for(i=0; i<5; i++){
       for(j=0; j<5; j++){
           cin>>a[i][j];
       }
   }
   for(i=0; i<5; i++){
       for(j=0; j<5; j++){
           if(a[i][j]==1){
               x=i+1;
               y=j+1;
           }
       }
   }
   ans=abs(x-3)+abs(y-3);
   cout<<ans;
}
