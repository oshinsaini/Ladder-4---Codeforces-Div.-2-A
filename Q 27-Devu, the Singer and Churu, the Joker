#include <iostream>
#include <bits/stdc++.h>
using namespace std;
int main() 
{
    int n; int d; cin>>n>>d;
    int arr[n]; int sum=0;
    for(int i=0; i<n; i++)
    {
        cin>>arr[i]; 
        sum+=arr[i];  
    }
    if(d-10*(n-1)-sum<0) cout<<"-1"<<endl;
    else
    {d-=sum;
    cout<<d/5<<endl;
    }
}
